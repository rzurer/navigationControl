extends layout 
block content
	.navigatorContainer
		img#navigateFirst(src='/images/first.png')
		img#navigatePrevious(src='/images/previous.png')
		img#navigateNext(src='/images/next.png')
		img#navigateLast(src='/images/last.png')
		img#removeCurrent(src='/images/remove.png')
	.controlsContainer
		label Set Source Array Length: 
		select#setSourceArraySelect
			option 0
			option 1
			option 2
			option 3
			option 4
			option 5
		label Current Index:
		label#currentIndexOutput.output
		label Source Array Length:
		label#sourceArrayLengthOutput.output
	script
		initializeNavigator();